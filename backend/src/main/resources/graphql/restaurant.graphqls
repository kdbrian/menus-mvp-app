type Restaurant {
    id :ID!
    name : String
    bannerImage    : String
    level : Int
    lng : Float
    lat : Float
    zipCode : String
    postalCode : String
    locationName    : String
    tagLine    : String
    thumbsUp : Float
    thumbsDown : Float
}

input RestaurantInput {
    name : String
    level : Int
    lat : Float
    lng : Float
    zipCode : String
    postalCode : String
    locationName    : String
    tagLine    : String
}
